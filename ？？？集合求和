//本来抖了个机灵用了高中集合知识，结果不知道为啥第一个总是-16，不管我输几qwq好难受

//这是错误代码
#include<bits/stdc++.h>
using namespace std;

int main() {
	string s;
	getline(cin,s);
	int siz=0;
	int num[30];
	for(int i=0;i<s.size();i++) {
		if(s[i]<='9'&&s[i]>='0') siz++;
		num[siz]=(int)s[i]-'0';
	}
	int p=1; 
	for(int j=1;j<=siz-1;j++) {
		p*=2;
	}
	int sum=0;
	for(int i=1;i<=siz;i++) {
		sum+=num[i]*p;
	}
	printf("%d\n",sum);
	return 0;
}		

